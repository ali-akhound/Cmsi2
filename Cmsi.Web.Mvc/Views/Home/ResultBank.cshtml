@model AVA.Web.Mvc.Models.InvoiceOnlineResultViewModel
<update-title title="{{'ResultBank' | translate}}"></update-title>
<update-meta property="og:title" content="{{'ResultBank' | translate}}"></update-meta>
<update-meta name="description" content="{{'HeaderTitle'| translate}}-{{'ResultBank' | translate}}"></update-meta>
<update-meta itemprop="description" content="{{'HeaderTitle'| translate}}-{{'ResultBank' | translate}}"></update-meta>
<update-meta name="keywords" content="{{'HeaderTitle'| translate}},{{'ResultBank' | translate}}"></update-meta>
<update-script content="structuredData" type="application/ld+json"></update-script>
<div id="content">
    <div class="container  animated fadeIn animation-delay-7">
        <h3 class="title-divider">
            <span>{{'ResultBank' |translate}}</span>
        </h3>
        <div class="row">
            <div class="col-md-12 col-xl-12">
                <div class="form-group">
                    <label for="inputPayment" class="col-md-2 control-label">{{'status' | translate}}:</label>
                    <div class="col-md-6">
                        @Model.StatusMsg
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <label for="inputPayment" class="col-md-2 control-label">{{'PaymentID' | translate}}:</label>
                    <div class="col-md-6">
                        @Model.paymentId
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group">
                    <label for="inputFName" class="col-md-2 control-label">{{'digitalCode' | translate}}:</label>
                    <div class="col-md-6">
                        @Model.referenceId
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>


